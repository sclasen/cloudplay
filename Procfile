web:           target/universal/stage/bin/cloudplay -Dlogger.file=conf/logger.xml -Dhttp.port=${PORT} ${JAVA_OPTS} -Dapplication.global=Web
continuous:    target/universal/stage/bin/cloudplay -Dlogger.file=conf/logger.xml -Dhttp.port=${PORT} ${JAVA_OPTS} -Dapplication.global=Continuous
migration:     target/universal/stage/bin/cloudplay -Dlogger.file=conf/logger.xml -Dhttp.port=${PORT} ${JAVA_OPTS} -Dapplication.global=Migration

